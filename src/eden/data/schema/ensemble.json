{
    "$id": "ensemble",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
      "version": {
        "type": "string",
        "description": "Eden library version"
      },
      "input_len": {
        "type": "integer"
      },
      "eden_leaf_indicator": {
        "type": "integer",
        "description": "Value in feature that denotes a leaf"
      },
      "output_len": {
        "type": "integer"
      },
      "n_estimators": {
        "type": "integer"
      },
      "n_trees": {
        "type": "integer"
      },
      "estimator": {
        "type": "string"
      },
      "is_forest": {
        "type": "boolean"
      },
      "is_classification": {
        "type": "boolean"
      },
      "n_nodes": {
        "type": "integer"
      },
      "n_leaves": {
        "type": "integer"
      },
      "max_depth": {
        "type": "integer"
      },
      "trees" : {
        "type":"array",
        "description": "List of trees",
        "items": {
            "$ref": "tree"
        }
      }
    },
    "required": [
      "version",
      "estimator",
      "input_len",
      "output_len",
      "is_classification",
      "n_nodes",
      "n_leaves",
      "n_trees",
      "max_depth",
      "trees",
      "n_estimators",
      "eden_leaf_indicator"
    ],
    "additionalProperties": false
  }