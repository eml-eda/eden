{
    "$id": "tree",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
      "version": {
        "type": "string",
        "description": "Eden library version"
      },
      "estimator": {
        "type": "string"
      },
      "is_classification": {
        "type": "boolean"
      },
      "input_len": {
        "type": "integer"
      },
      "eden_leaf_indicator": {
        "type": "integer"
      },
      "leaf_len": {
        "type": "integer"
      },
      "n_nodes": {
        "type": "integer"
      },
      "n_leaves": {
        "type": "integer"
      },
      "max_depth": {
        "type": "integer"
      },
      "feature": {
        "type": "array",
        "items": {
          "type": "integer"
        },
        "description": "Feature index of the nodes"
      },
      "threshold": {
        "type": "array",
        "items": {
          "type": "number"
        },
        "description": "Thresholds of nodes"
      },
      "children_right": {
        "type": "array",
        "items": {
          "type": "integer"
        }
      },
      "children_left": {
        "type": "array",
        "items": {
          "type": "integer"
        }
      },
      "value": {
        "type": "array",
        "items": {
          "type": "array",
          "items": {
            "type": "number"
          }
        }
      }
    },
    "required": [
      "version",
      "estimator",
      "is_classification",
      "input_len",
      "n_nodes",
      "n_leaves",
      "max_depth",
      "feature",
      "threshold",
      "children_right",
      "children_left",
      "value",
      "leaf_len",
      "eden_leaf_indicator"
    ],
    "additionalProperties": false
  }